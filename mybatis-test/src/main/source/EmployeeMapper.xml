<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
  namespace: 名称空间
  id: 唯一标识
  resultType: 返回值类型
  #{id}: 从传递过来的参数中取出 id 值
-->
<mapper namespace="top.xiesen.mybatis.EmployeeMapper">
    <select id="selectEmp" resultType="top.xiesen.mybatis.bean.Employee">
select id,last_name lastName,gender,email from tb_employee where id = #{id}
</select>
</mapper>